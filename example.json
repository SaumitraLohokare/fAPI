{
    "port": 8080,
    "routes": [
        {
            "url": "/hello",
            "GET": {
                "status": 200,
                "response": {
                    "Content-Type": "plaintext",
                    "value": "Hello, World"
                }
            },
            "POST": {
                "status": 200,
                "response": {
                    "Content-Type": "json",
                    "value": "{ \"id\": \"1234567890\" }"
                }
            }
        },
        {
            "url": "/users/:id",
            "GET": {
                "status": 200,
                "response": {
                    "Content-Type": "json",
                    "value": "{ \"id\": \"${url.id}\", \"name\": \"${random_name()}\", \"age\": \"${random_int(18, 50)}\" }"
                }
            }
        }
    ]
}